

--Caso Pedido: 1-8852919442   PLANID: 4163958
--Se trasladan cuotas vencidas
update USRATVPRD.AC_DPLANPAGO 
 set FECHAPAGO=ADD_MONTHS(FECHAPAGO,21)
 WHERE PLANID = 4163958
   AND TIPODET = 0 
   AND ESTADO = 1
   AND ACCION_BRM = 'AJUSTADO' 
   AND trunc(FECHAPAGO) < sysdate ;

--Se actualiza el plan de cuotas
update USRATVPRD.AC_DPLANPAGO 
 set  FECHA_BRM = NULL 
     ,ACCION_BRM = NULL 
     ,IDEVENT = NULL  
     ,MSG = NULL 
     ,PROCESO_COMPRA = NULL 
     ,BATCHID_COMPRA = NULL 
     ,CUOTAS_CAMBIOPLAN=0  
     ,ESTADO=0 
     ,NUMFACT=NULL 
     ,FREALPAGO=NULL 
 WHERE PLANID = 4163958
   AND TIPODET = 0 
   AND FECHAPAGO >= sysdate 
   AND ESTADO = 1
   AND ACCION_BRM = 'AJUSTADO' ;

commit;
