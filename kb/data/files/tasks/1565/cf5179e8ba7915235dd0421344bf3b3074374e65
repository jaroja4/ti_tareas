-----------------TABLAS----------------------

  CREATE TABLE USRAUDISIMOPRD.AUDITORIA_HISTORICO 
   (	PK_AUDITORIA_HIST NUMBER(19,0) NOT NULL ENABLE, 
	VERSION NUMBER(19,0) NOT NULL ENABLE, 
	CODIGO_AUDITORIA_HIST NUMBER(10,0) NOT NULL ENABLE, 
	DESCRIPCION_AUDITORIA_HIST VARCHAR2(300 CHAR), 
	FECHA_INGRESO_AUDITORIA_HIST TIMESTAMP (6), 
	 PRIMARY KEY (PK_AUDITORIA_HIST))
  TABLESPACE AUDISIMO_HIST ;


 CREATE TABLE USRAUDISIMOPRD.CAMPO_HISTORICO
   (	PK_CAMPO_HIST NUMBER(19,0) NOT NULL ENABLE, 
	VERSION NUMBER(19,0) NOT NULL ENABLE, 
	AUDITORIA_HISTORICO_ID NUMBER(19,0) NOT NULL ENABLE, 
	CAMPO_CONF_CAMPO_HIST NUMBER(19,0), 
	CODIGO_AUDITORIA_CAMPO_HIST NUMBER(10,0) NOT NULL ENABLE, 
	DESCRIPCION_CAMPO_HIST VARCHAR2(255 CHAR), 
	TIPO_CAMPO_HIST NUMBER(10,0), 
	VALOR_CAMPO_HIST VARCHAR2(2000 CHAR), 
	CAMPOS_IDX NUMBER(10,0), 
	 PRIMARY KEY (PK_CAMPO_HIST),
	CONSTRAINT AUDITORIA_HISTORICO_ID_FK FOREIGN KEY (AUDITORIA_HISTORICO_ID)
	  REFERENCES USRAUDISIMOPRD.AUDITORIA_HISTORICO (PK_AUDITORIA_HIST) ENABLE
   ) 
  TABLESPACE AUDISIMO_HIST ;

--------------INDICES------------------------

CREATE INDEX USRAUDISIMOPRD.IDX_FECHA__HISTORICO ON USRAUDISIMOPRD.AUDITORIA_HISTORICO (FECHA_INGRESO_AUDITORIA_HIST)
 TABLESPACE AUDISIMO_HIST ;

CREATE INDEX USRAUDISIMOPRD.IDX_AUDITORIA_HIST_ID ON USRAUDISIMOPRD.CAMPO_HISTORICO (AUDITORIA_HISTORICO_ID) 
  TABLESPACE AUDISIMO_HIST ;
 
CREATE INDEX USRAUDISIMOPRD.IDX_CAMPO_HIST_CONF_ID ON USRAUDISIMOPRD.CAMPO_HISTORICO (CAMPO_CONF_CAMPO_HIST) 
  TABLESPACE AUDISIMO_HIST ;
 
CREATE INDEX USRAUDISIMOPRD.IDX_VALOR_HIST ON USRAUDISIMOPRD.CAMPO_HISTORICO (VALOR_CAMPO_HIST) 
  TABLESPACE AUDISIMO_HIST ;

---------------SECUENCIAS---------------------

CREATE SEQUENCE  USRAUDISIMOPRD.AUDITORIA_HISTORICO_SEQ  MINVALUE 1 MAXVALUE 999999999999999999999999999 INCREMENT BY 1 START WITH 10679003 CACHE 20 NOORDER  NOCYCLE ;
CREATE SEQUENCE  USRAUDISIMOPRD.CAMPO_HISTORICO_SEQ  MINVALUE 1 MAXVALUE 999999999999999999999999999 INCREMENT BY 1 START WITH 160163711 CACHE 20 NOORDER  NOCYCLE ;
 




